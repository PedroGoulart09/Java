package EnumeracoesEcomposicao;

import java.util.Date;
import java.util.Scanner;

public class EnumeracoesEcomposicao {
	
	 public static double teste(double testando) {
		 return testando + 1;
	 }
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		/*
		 * Order enu = new Order(1080, new Date(), OrderStatus.PENDING_PAYMENT);
		 * System.out.println(enu);
		 * 
		 * OrderStatus s1 = OrderStatus.DELIVERED; OrderStatus s2 =
		 * OrderStatus.valueOf("DELIVERED");
		 * 
		 * System.out.println(s1); System.out.println(s2);
		 */
		
		System.out.println("Enter number: ");
		double tes = sc.nextDouble();
		System.out.printf("%.0f", teste(tes), tes);
		
		sc.close();
		
		
		
	}
}
